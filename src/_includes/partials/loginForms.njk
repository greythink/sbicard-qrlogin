                                    onclick="proceedCardLogin('/creditcards/app/user/validate-card-number')"
                                    class="btn waves-effect waves-light blue-btn disable-btn"
                                    id="loginProceedBtnCard">Proceed</a>
                            </div>
                            <div
                                id="loginCardSubmitBtn"
                                class="input-field mt-30 add-disable"
                                style="display:none">
                                <a
                                    type="button"
                                    id="loginGenOtpCard"
                                    class="btn waves-effect waves-light blue-btn disable-btn"
                                    onclick="genCardOtp('/creditcards/app/user/login-card-otp-gen');">
