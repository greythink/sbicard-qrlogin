                                <img
                                    class="icon icon1"
                                    id="login-dob-cross"
                                    style="display:none"
                                    src="/creditcards/resources/img/field-error.png">
                                <input
                                    class="sendOtp dummy-card-dob-width100"
                                    id="dummy-card-dob"
                                    type="text"
                                    onclick="validateImageCheckboxcheckedOrNot('cardlogin')"
                                    required
                                    readonly/>
                                <label id="dummy-login-dob-label" for="dummy-card-dob">Date of Birth
                                    (DD-MMM-YYYY)</label>
                                <span
                                    id="card_calendar_error"
                                    class="helper-text"
                                    data-success="right">Please verify your Authentication Image & Text
