                                <span class="muted">Recommended for secure login</span>
                                <div class="wrapper ">
                                    <div class="switch-div">
                                        <label class="switch">
                                            <input type="checkbox"/>
                                            <div class="switch-btn"></div>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div
                                id="card-login-proceed-button-section"
                                class="input-field mt-30 add-disable">
